{
    "name": "internetsystemsdev",
    "version": "1.0.0",
    "description": "My first Web App",
    "main": "app.js",
    "scripts": {
        "test": "tsc && jest --detectOpenHandles --forceExit ./dist/src/tests",
        "start": "tsc && node dist/src/app.js",
        "lint": "eslint . --ext .ts",
        "devstart": "nodemon ./src/app.ts",
        "testAuth": "tsc && jest --detectOpenHandles --forceExit --runInBand ./dist/src/tests/auth.test.js",
        "testPost": "tsc && jest --detectOpenHandles --forceExit --runInBand ./dist/src/tests/post.test.js",
        "testSocket": "tsc && jest --detectOpenHandles --forceExit --runInBand ./dist/src/tests/socket.test.js",
        "testStudent": "tsc && jest --detectOpenHandles --forceExit --runInBand ./dist/src/tests/student.test.js",
        "testFile": "tsc && jest --detectOpenHandles --forceExit --runInBand ./dist/src/tests/file.test.js"
    },
    "author": "Yarden Hovav",
    "license": "ISC",
    "dependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.19.6",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.1",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "form-data": "^4.0.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.7.2",
        "multer": "^1.4.5-lts.1",
        "mz": "^2.7.0",
        "nodemon": "^2.0.20",
        "socket.io": "^4.5.4",
        "socket.io-client": "^4.5.4"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.1",
        "@types/jest": "^29.2.2",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/supertest": "^2.0.12",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "eslint": "^8.27.0",
        "jest": "^29.2.2",
        "supertest": "^6.3.1",
        "swagger-jsdoc": "^6.2.5",
        "swagger-ui-express": "^4.6.0",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
